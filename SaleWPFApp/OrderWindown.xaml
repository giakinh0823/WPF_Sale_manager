<Window x:Class="SaleWPFApp.OrderWindown"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SaleWPFApp"
        mc:Ignorable="d"
        Title="Order" Height="450" Width="800">
    <Window.Resources>
        <ResourceDictionary Source="StyleResource.xaml"></ResourceDictionary>
    </Window.Resources>
    <Grid Background="White">
        <TextBlock Margin="0 0 0 50"  FontWeight="Light" FontSize="50" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{StaticResource ResourceKey=headingPrimary}">
              Welcome to FStore Order
        </TextBlock>
    </Grid>
</Window>
