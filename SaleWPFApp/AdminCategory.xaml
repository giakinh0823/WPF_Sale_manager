<Page x:Class="SaleWPFApp.AdminCategory"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SaleWPFApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminCategory">

    <Page.Resources>
        <ResourceDictionary Source="StyleResource.xaml"></ResourceDictionary>
    </Page.Resources>
    <Grid Background="White">
        <TextBlock Margin="0 0 0 50"  FontWeight="Light" FontSize="50" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{StaticResource ResourceKey=headingPrimary}">
              Welcome to FStore Admin Category
        </TextBlock>
    </Grid>
</Page>
